{
  "name": "@seneca/weaviate-store",
  "version": "0.0.1",
  "description": "Seneca Weaviate data storage plugin.",
  "main": "dist/WeaviateStore.js",
  "type": "commonjs",
  "types": "dist/WeaviateStore.d.ts",
  "license": "MIT",
  "author": "Richard Rodger (http://richardrodger.com)",
  "contributors": [
    "Richard Rodger (http://richardrodger.com)"
  ],
  "scripts": {
    "test": "jest --coverage",
    "test-some": "jest -t",
    "test-watch": "jest --coverage --watchAll",
    "watch": "tsc -w -d",
    "build": "tsc -d",
    "doc": "seneca-doc -p seneca-entity",
    "prettier": "prettier --write --no-semi --single-quote src/**/*.ts test/*.js",
    "reset": "npm run clean && npm i && npm run build && npm test",
    "clean": "rm -rf node_modules dist package-lock.json yarn.lock",
    "repo-tag": "REPO_VERSION=`node -e \"console.log(require('./package').version)\"` && echo TAG: v$REPO_VERSION && git commit -a -m v$REPO_VERSION && git push && git tag v$REPO_VERSION && git push --tags;",
    "repo-publish": "npm run clean && npm i --registry=http://registry.npmjs.org && npm run repo-publish-quick",
    "repo-publish-quick": "npm run prettier && npm run build && npm run doc && npm test && npm run repo-tag && npm publish --access public --registry=https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/senecajs/SenecaWeaviateStore"
  },
  "keywords": [
    "seneca",
    "plugin",
    "store",
    "mem",
    "memory"
  ],
  "peerDependencies": {
    "@seneca/entity-util": ">=2",
    "seneca": ">=3",
    "seneca-entity": ">=25",
    "seneca-promisify": ">=3"
  },
  "devDependencies": {
    "@seneca/doc": "^7.2.0",
    "@seneca/maintain": "^0.1.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.4",
    "dotenv": "^16.4.5",
    "esbuild": "^0.20.2",
    "esbuild-jest": "^0.5.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "seneca-msg-test": "^4.1.0",
    "seneca-store-test": "^5.2.0",
    "typescript": "^5.4.4"
  },
  "files": [
    "README.md",
    "CHANGES.md",
    "LICENSE",
    "src",
    "dist"
  ],
  "dependencies": {
    "weaviate-client": "^3.0.0-beta.22"
  }
}
